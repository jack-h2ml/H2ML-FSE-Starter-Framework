{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		}, {
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": false,
			"palette": [
				{
					"color": "#EEEAE7",
					"name": "Base",
					"slug": "base"
				}, {
					"color": "#222222",
					"name": "Contrast",
					"slug": "contrast"
				}, {
					"color": "#E3E1D7",
					"name": "Base Darker 20",
					"slug": "primary"
				}, {
					"color": "#AAA584",
					"name": "Base Darker 30",
					"slug": "secondary"
				}, {
					"color": "#887D40",
					"name": "Base Darker 50",
					"slug": "tertiary"
				}
			],
			"link": true
		},
		"layout": {
			"contentSize": "900px",
			"wideSize": "1200px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"spacingSizes": [
				{
					"size": "clamp(8px, 1.2vw, 16px)",
					"slug": "10",
					"name": "1"
				}, {
					"size": "clamp(12px, 1.8vw, 24px)",
					"slug": "20",
					"name": "2"
				}, {
					"size": "clamp(16px, 2.4vw, 32px)",
					"slug": "30",
					"name": "3"
				}, {
					"size": "clamp(20px, 3vw, 40px)",
					"slug": "40",
					"name": "4"
				}, {
					"size": "clamp(24px, 3.6vw, 48px)",
					"slug": "50",
					"name": "5"
				}, {
					"size": "clamp(28px, 4.2vw, 52px)",
					"slug": "60",
					"name": "6"
				}, {
					"size": "clamp(32px, 5.8vw, 64px)",
					"slug": "70",
					"name": "7"
				}, {
					"size": "clamp(36px, 6.4vw, 72px)",
					"slug": "80",
					"name": "8"
				}, {
					"size": "clamp(40px, 7vw, 80px)",
					"slug": "90",
					"name": "9"
				}, {
					"size": "clamp(44px, 7.6vw, 88px)",
					"slug": "100",
					"name": "10"
				}, {
					"size": "clamp(48px, 8.2vw, 96px)",
					"slug": "110",
					"name": "11"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fontFamilies": [{
				"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
				"name": "System Font",
				"slug": "system-font"
			}],
			"fontSizes": [
				{
					"size": "clamp(0.6rem, 0.27vw + 0.55rem, 0.75rem)",
					"slug": "small",
					"name": "Small"
				}, {
					"size": "clamp(0.75rem, 0.45vw + 0.66rem, 0.9rem)",
					"slug": "medium",
					"name": "Medium"
				},{
					"size": "clamp(0.86rem, 0.52vw + 0.71rem, 1.1rem)",
					"slug": "medium-l",
					"name": "Big"
				}, {
					"size": "clamp(0.94rem, 0.72vw + 0.79rem, 1.33rem)",
					"slug": "large",
					"name": "Large"
				}, {
					"size": "clamp(1.17rem, 1.1vw + 0.95rem, 1.78rem)",
					"slug": "x-large",
					"name": "Extra Large"
				}, {
					"size": "clamp(1.69rem, 1.48vw + 0.97rem, 2.5rem)",
					"slug": "xx-large",
					"name": "Huge"
				}, {
					"size": "clamp(2.1rem, 1.7vw + 1.3rem, 5rem)",
					"slug": "xxx-large",
					"name": "Gigantic"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--20)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--20)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--20)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--courier)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": true
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		}, {
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}, {
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		}, {
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	]
}